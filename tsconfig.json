{
 "compilerOptions": {
   "target": "ES2020",
   "module": "NodeNext",
   "moduleResolution": "NodeNext",
   "outDir": "./dist",
   "strict": true,
   "esModuleInterop": true,
   "skipLibCheck": true,
   "typeRoots": ["./node_modules/@types", "./src/types"],
   "types": ["express", "multer"]
 },
 "include": ["src/**/*"],
 "exclude": ["node_modules"]
}